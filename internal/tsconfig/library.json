{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Library TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": false,
    "moduleResolution": "bundler",
    "declaration": true,
    "noEmit": false
  }
}
