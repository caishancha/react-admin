{
  "name": "@react-admin/tailwindcss-config",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "caishancha",
  "license": "MIT",
  "scripts": {
    "stub": "pnpm unbuild --stub"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "import": "./dist/config/index.mjs",
      "default": "./dist/config/index.mjs"
    },
    "./tailwind.css": {
      "default": "./src/css/index.css"
    }
  },
  "dependencies": {
    "@iconify/json": "catalog:",
    "@iconify/tailwind4": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "tailwindcss": "catalog:"
  }
}
