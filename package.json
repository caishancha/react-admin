{
  "name": "react-admin",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "keywords": [
    "typescript",
    "react",
    "react-admin",
    "monorepo",
    "turbo"
  ],
  "description": "react-admin",
  "author": "caishancha",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "commit": "czg",
    "clean": "node ./scripts/clean.mjs",
    "dev": "turbo run dev",
    "dev:antd": "pnpm --filter=@react-admin/web-antd run dev ",
    "lint": "pnpm run lint:fix",
    "lint:fix": "vsh lint --format",
    "lint:check": "vsh lint",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky || true",
    "publint": "vsh publint",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "catalog": "pnpx codemod pnpm/catalog"
  },
  "devDependencies": {
    "@react-admin/commitlint-config": "workspace:*",
    "@react-admin/eslint-config": "workspace:*",
    "@react-admin/prettier-config": "workspace:*",
    "@react-admin/stylelint-config": "workspace:*",
    "@react-admin/tailwindcss-config": "workspace:*",
    "@react-admin/ts-config": "workspace:*",
    "@react-admin/vite-config": "workspace:*",
    "@react-admin/vsh": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "cross-env": "catalog:",
    "cspell": "catalog:",
    "happy-dom": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:",
    "tsdown": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@10.13.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "neverBuiltDependencies": [
      "canvas"
    ]
  }
}
