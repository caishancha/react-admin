{
  "name": "react-admin",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "keywords": [
    "typescript",
    "react",
    "react-admin",
    "monorepo",
    "turbo"
  ],
  "description": "react-admin",
  "author": "caishancha",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "check-types": "tsc --noEmit",
    "clean": "node ./scripts/clean.mjs",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "devDependencies": {
    "@react-admin-core/shadcn-ui": "workspace:*",
    "@react-admin/prettier-config": "workspace:*",
    "husky": "catalog:",
    "turbo": "catalog:",
    "unbuild": "catalog:"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@10.13.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "neverBuiltDependencies": [
      "canvas"
    ]
  }
}
